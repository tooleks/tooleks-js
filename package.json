{
  "name": "tooleks",
  "version": "2.1.2",
  "description": "A set of tools for JavaScript development",
  "keywords": [
    "utils",
    "inversion of control",
    "dependency injection",
    "dependency container",
    "dependency injection container",
    "event emitter",
    "asynchronous event emitter",
    "data mapper",
    "async utils",
    "typescript"
  ],
  "browser": "./dist/main.umd.js",
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c && tsc -p ./tsconfig.declaration.json",
    "lint": "tslint --fix -p ./tsconfig.json -c ./tslint.json",
    "test": "mocha -r ts-node/register ./tests/**/*.spec.ts",
    "coverage": "nyc npm run test",
    "docs": "typedoc --out ./docs ./src/**/*.ts",
    "pre-publish": "npm run lint && npm run coverage && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/tooleks-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/tooleks-js/issues"
  },
  "homepage": "https://tooleks.github.io/tooleks-js",
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/chai": "^4.2.11",
    "@types/core-js": "^2.5.3",
    "@types/faker": "^4.1.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "faker": "^4.1.0",
    "mocha": "^7.1.1",
    "moment": "^2.24.0",
    "nyc": "^15.0.1",
    "rollup": "^2.6.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.1",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
