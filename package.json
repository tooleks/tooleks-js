{
  "name": "tooleks",
  "version": "1.4.6",
  "description": "A set of tools for JavaScript development",
  "keywords": [
    "defer",
    "dependency",
    "container",
    "dependency-container",
    "injection",
    "dependency-injection",
    "binding",
    "singleton",
    "factory",
    "instance",
    "event",
    "emitter",
    "event-emitter",
    "mapper",
    "clone",
    "optional",
    "utils",
    "async",
    "asyncronous"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha './tests/**/*-test.js'",
    "formatter": "./node_modules/.bin/prettier --write '**/*.js' '!dist/**'",
    "build-preparation": "npm run formatter && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/tooleks-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/tooleks-js/issues"
  },
  "homepage": "https://github.com/tooleks/tooleks-js#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "rollup": "^0.63.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sinon": "^4.5.0"
  }
}
