{
  "name": "tooleks",
  "version": "1.4.9",
  "description": "A set of tools for JavaScript development",
  "keywords": [
    "defer",
    "dependency",
    "container",
    "dependency-container",
    "injection",
    "dependency-injection",
    "binding",
    "singleton",
    "factory",
    "instance",
    "event",
    "emitter",
    "event-emitter",
    "mapper",
    "clone",
    "optional",
    "utils",
    "async",
    "asyncronous"
  ],
  "main": "./dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config './webpack/webpack.config.js'",
    "test": "./node_modules/.bin/mocha -r esm './tests/main-test.js'",
    "format": "./node_modules/.bin/prettier --write '**/*.js' '!dist/**'",
    "build-release": "npm run format && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooleks/tooleks-js.git"
  },
  "author": "Oleksandr Tolochko <tooleks@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooleks/tooleks-js/issues"
  },
  "homepage": "https://github.com/tooleks/tooleks-js#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.0",
    "esm": "^3.0.72",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "prettier": "^1.13.7",
    "sinon": "^4.5.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  }
}
